		<item name="android:layout_width">275dp</item>
		<item name="android:layout_height">175dp</item>
	</style>

	<style name="CardView.Box.Clean">
		<item name="android:layout_width">350dp</item>
		<item name="android:layout_height">250dp</item>
	</style>

