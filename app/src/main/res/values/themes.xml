		<item name="tint">@color/md_theme_light_onPrimary</item>
	</style>

	<style name="DetailHeaderText">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>

		<item name="android:textColor">@color/md_theme_light_onPrimary</item>
		<item name="autoSizeTextType">uniform</item>
	</style>

	<style name="DetailHeaderText.Title">
		<item name="android:layout_marginStart">15dp</item>
		<item name="android:layout_marginBottom">10dp</item>

		<item name="android:textSize">30sp</item>
		<item name="android:fontFamily">@font/poppins_semibold</item>
	</style>

	<style name="DetailHeaderText.Content">
		<item name="android:layout_marginStart">15dp</item>

		<item name="android:textSize">14sp</item>
		<item name="android:fontFamily">@font/poppins_regular</item>
	</style>

	<style name="CardView">
